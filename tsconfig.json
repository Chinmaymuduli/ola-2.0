// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */,
    "baseUrl": "./",
    "paths": {
      "api": [
        "src/api"
      ],
      "app": [
        "src/app"
      ],
      "assets": [
        "src/assets"
      ],
   
      "components": [
        "src/components"
      ],
    
      "configs": [
        "src/configs"
      ],     
      "constant": [
        "src/constant"
      ],
      "hooks": [
        "src/hooks"
      ],
      "layouts": [
        "src/layouts"
      ],
      "routes": [
        "src/routes"
      ],
      "schemas": [
        "src/schemas"
      ],
      "screens": [
        "src/screens"
      ],
      "styles": [
        "src/styles"
      ],
      "utils": [
        "src/utils"
      ],
      "types": [
        "src/types"
      ]
    },
  }
}
